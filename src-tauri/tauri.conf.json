{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../build",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "category": "Business",
    "copyright": "AHQ Softwares",
    "targets": ["msi", "updater", "deb", "rpm", "appimage"],
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "allowDowngrades": true,
      "wix": {
        "template": "assets/install.wxs"
      }
    },
    "longDescription": "AHQ Store\nA New Store for downloading apps!",
    "resources": ["icons/pwd_reset.png"],
    "shortDescription": "AHQ Store",
    "linux": {
      "deb": {
        "depends": ["fuse", "libfuse2"]
      },
      "appimage": {
        "bundleMediaFramework": true,
        "files": {}
      }
    },
    "fileAssociations": [
      {
        "description": "The AHQ Store binary file",
        "ext": [".ahqstore", ".astore"],
        "mimeType": "application/ahqstore",
        "name": "AHQ Store File",
        "role": "Viewer"
      }
    ]
  },
  "productName": "AHQ Store",
  "version": "../package.json",
  "identifier": "com.ahqstore.app",
  "app": {
    "windows": [
      {
        "center": true,
        "height": 760,
        "resizable": true,
        "title": "AHQ Store",
        "label": "main",
        "width": 1240,
        "minHeight": 700,
        "minWidth": 1200,
        "visible": false,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
